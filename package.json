{
  "name": "@vitesse-lite/template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 3333 --open",
    "build": "cross-env NODE_ENV=production vite build",
    "ls-lint":"ls-lint",
    "lint": "eslint --ext .js,.ts,.vue src/",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue src/"
  },
  "gitHooks": {
    "pre-commit": "ls-lint && lint-staged",
    "commit-msg": "node scripts/verify-commit.js"
  },
  "lint-staged": {
    "nopackages/**/{src,__tests__,config}/**/**.{js,vue,ts,tsx}": [
      "eslint --ext .js,.vue,.ts --fix"
    ]
  },
  "dependencies": {
    "@vueuse/core": "^4.0.0-beta.11",
    "vue": "^3.0.0-rc.10",
    "vue-router": "^4.0.0-beta.12"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.0-rc.10",
    "cross-env": "^7.0.2",
    "lint-staged": "^10.5.3",
    "eslint-config-standard-vue-ts": "^1.0.8",
    "prettier": "^2.1.2",
    "tailwindcss": "^1.8.10",
    "typescript": "^4.0.2",
    "vite": "^1.0.0-rc.4",
    "vite-plugin-components": "^0.2.13",
    "vite-plugin-voie": "^0.4.0",
    "voie-pages": "^0.4.0",
    "yorkie": "^2.0.0"
  }
}
