{
  "name": "normojs",
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run normo",
    "dev:open": "vite --port 3333 --open",
    "build": "cross-env NODE_ENV=production vite build",
    "ls-lint": "ls-lint",
    "lint": "eslint --ext .js,.ts,.vue src/",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue src/",
    "normo": "vite --port 3333 --config normo.config.ts"
  },
  "lint-staged": {
    "nopackages/**/{src,__tests__,config}/**/**.{js,vue,ts,tsx}": [
      "eslint --ext .js,.vue,.ts --fix"
    ]
  },
  "dependencies": {
    "@vueuse/core": "^4.0.0-beta.11",
    "vue": "^3.0.0-rc.10",
    "vue-router": "^4.0.0-beta.12",
    "nprogress": "^0.2.0"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.0-rc.10",
    "@vitejs/plugin-vue": "^1.1.0",
    "@types/nprogress": "^0.2.0",
    "@types/node": "^14.14.22",
    "cross-env": "^7.0.2",
    "lint-staged": "^10.5.3",
    "eslint-config-standard-vue-ts": "^1.0.8",
    "prettier": "^2.1.2",
    "tailwindcss": "^1.8.10",
    "typescript": "^4.0.2",
    "vite": "^2.0.0-beta.35",
    "vite-plugin-components": "^0.6.6",
    "vite-plugin-pages": "^0.1.4",
    "vite-ssg": "^0.4.1",
    "yorkie": "^2.0.0"
  }
}
